# Compute the installation prefix from this PyVinecopulibConfig.cmake file location.
get_filename_component(PYVINECOPULIB_INSTALL_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(PYVINECOPULIB_VERSION_MAJOR "@PYVINECOPULIB_VERSION_MAJOR@")
set(PYVINECOPULIB_VERSION_MINOR "@PYVINECOPULIB_VERSION_MINOR@")
set(PYVINECOPULIB_VERSION_PATCH "@PYVINECOPULIB_VERSION_PATCH@")
set(PYVINECOPULIB_BUILD_SHARED "@BUILD_SHARED_LIBS@")
set(PYVINECOPULIB_LIBRARIES "@PYVINECOPULIB_LIBRARIES@")
set(PYVINECOPULIB_C_FLAGS "@CMAKE_C_FLAGS@")
set(PYVINECOPULIB_CXX_FLAGS "@CMAKE_CXX_FLAGS@")
set(PYVINECOPULIB_EXE_LINKER_FLAGS "@CMAKE_EXE_LINKER_FLAGS@")
set(PYVINECOPULIB_SHARED_LINKER_FLAGS "@CMAKE_SHARED_LINKER_FLAGS@")
set(PYVINECOPULIB_MODULE_LINKER_FLAGS "@CMAKE_MODULE_LINKER_FLAGS@")

set(PYVINECOPULIB_INCLUDE_DIRS "${PYVINECOPULIB_INSTALL_PREFIX}/@PYVINECOPULIB_INSTALL_INC_DIR@")
set(PYVINECOPULIB_LIBRARY_DIRS "${PYVINECOPULIB_INSTALL_PREFIX}/@PYVINECOPULIB_INSTALL_LIB_DIR@")
set(PYVINECOPULIB_README_FILE  "${PYVINECOPULIB_INSTALL_PREFIX}/README.md")
set(PYVINECOPULIB_LICENSE_FILE "${PYVINECOPULIB_INSTALL_PREFIX}/LICENSE.txt")

set(PYVINECOPULIB_USE_FILE "${PYVINECOPULIB_INSTALL_PREFIX}/UsePyVinecopulib.cmake")

#Â See: https://www.stack.nl/~dimitri/doxygen/manual/external.html
# And: I'm assuming this would only be used in Build folder.
set(PYVINECOPULIB_DOXYGEN_TAG_PREFIX "Doxygen/PYVINECOPULIB.tag")
set(PYVINECOPULIB_DOXYGEN_TAG_FILE "@CMAKE_CURRENT_BINARY_DIR@/Doxygen/PYVINECOPULIB.tag")
